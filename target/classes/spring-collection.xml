<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

       <bean id="collectionDi" class="com.ioc.di.CollectionDi">
              <property name="set">
                     <set>
                            <value>set01</value>
                            <value>set02</value>
                            <value>set01</value>
                            <value>set02</value>
                     </set>
              </property>
              <property name="list">
                     <list>
                            <value>list01</value>
                            <value>list02</value>
                            <value>list01</value>
                            <value>list02</value>
                     </list>
              </property>
              <property name="map">
                     <map>
                            <entry key="1" value="map01" />
                            <entry key="2" value="map02" />
                            <entry key="3" value="map03" />
                            <entry key="4" value="map04" />
                     </map>
              </property>
              <property name="array">
                     <array>
                            <value>array01</value>
                            <value>array02</value>
                            <value>array01</value>
                            <value>array02</value>
                     </array>
              </property>
              <property name="properties">
                     <props>
                            <prop key="p1">props01</prop>
                            <prop key="p2">props02</prop>
                            <prop key="p3">props03</prop>
                            <prop key="p1">props01</prop>
                     </props>
              </property>
